@article{1gzp030,

author = {Potapov, Vladimir and Cohen, Mati and Schreiber, Gideon},

title = "{Assessing computational methods for predicting protein stability upon mutation: good on average but not in the details}",

journal = {Protein Engineering, Design and Selection},

volume = {22},

number = {9},

pages = {553-560},

year = {2009},

month = {06},

abstract = "{Methods for protein modeling and design advanced rapidly in recent years. At the heart of these computational methods is an energy function that calculates the free energy of the system. Many of these functions were also developed to estimate the consequence of mutation on protein stability or binding affinity. In the current study, we chose six different methods that were previously reported as being able to predict the change in protein stability (ΔΔG) upon mutation: CC/PBSA, EGAD, FoldX, I-Mutant2.0, Rosetta and Hunter. We evaluated their performance on a large set of 2156 single mutations, avoiding for each program the mutations used for training. The correlation coefficients between experimental and predicted ΔΔG values were in the range of 0.59 for the best and 0.26 for the worst performing method. All the tested computational methods showed a correct trend in their predictions, but failed in providing the precise values. This is not due to lack in precision of the experimental data, which showed a correlation coefficient of 0.86 between different measurements. Combining the methods did not significantly improve prediction accuracy compared to a single method. These results suggest that there is still room for improvement, which is crucial if we want forcefields to perform better in their various tasks.}",

issn = {1741-0126},

doi = {10.1093/protein/gzp030},

url = {https://doi.org/10.1093/protein/gzp030},

eprint = {http://oup.prod.sis.lan/peds/article-pdf/22/9/553/4354997/gzp030.pdf},

}

