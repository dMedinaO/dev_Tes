@article {Kishida4544,
	author = {Kishida, Takeshi and Stackhouse, Thomas M. and Chen, Fan and Lerman, Michael I. and Zbar, Berton},
	title = {Cellular Proteins That Bind the von Hippel-Lindau Disease Gene Product: Mapping of Binding Domains and the Effect of Missense Mutations},
	volume = {55},
	number = {20},
	pages = {4544--4548},
	year = {1995},
	publisher = {American Association for Cancer Research},
	abstract = {The von Hippel-Lindau disease (VHL) gene is a novel tumor suppressor gene that plays a role in the pathogenesis of renal cell carcinomas and hemangioblastomas of the central nervous system. To begin an evaluation of the biological functions of the VHL gene product (pVHL), we prepared bacterial fusion protein between glutathione S-transferase and wild-type or mutant pVHLs. The fusion proteins were used to identify cellular proteins that bind to pVHL in vitro. Monkey kidney cells transfected with wild-type or mutant VHL cDNAs were used to identify cellular proteins that bind to pVHL in vivo. Wild-type pVHL consistently bound two cellular proteins with apparent molecular masses of 10 and 14 kilodaltons that were designated p10 and p14, respectively. Mapping studies with a panel of VHL deletion mutant proteins demonstrated that p10 and p14 bound to a 32-amino acid peptide located in the carboxy terminal portion of pVHL. Missense mutations located within this 32-amino acid peptide abrogated the ability of the VHL protein to bind p10 and p14. Of 67 VHL families with identified germline mutations, 42 families had mutations predicted to affect the p10/p14-binding region. Maintenance of the integrity of the p10/p14-binding region appears to be essential for cellular growth regulation by pVHL. {\textcopyright}1995 American Association for Cancer Research.},
	issn = {0008-5472},
	URL = {http://cancerres.aacrjournals.org/content/55/20/4544},
	eprint = {http://cancerres.aacrjournals.org/content/55/20/4544.full.pdf},
	journal = {Cancer Research}
